<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lnyp.quickandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".sys.BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/no_title">
        <activity
            android:name=".page.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".page.GudieActivity" />

        <activity android:name=".page.BaseActivity" />

        <activity android:name=".page.MainActivity" />

    </application>

</manifest>
